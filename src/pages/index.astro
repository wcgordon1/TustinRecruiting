---
import BaseLayout from "@/layouts/BaseLayout.astro";
import HeaderOne from "@/components/headers/HeaderOne.astro";
import FeatureThree from "@/components/features/FeatureThree.astro";
import TestimonialTwo from "@/components/testimonials/TestimonialTwo.astro";
import PricingOne from "@/components/pricing/PricingOne.astro";
import FaqTwo from "@/components/faqs/FaqTwo.astro";
import CtaOne from "@/components/cta/CtaOne.astro";
import CompanyEntries from "@/components/company/CompanyEntries.astro";
import CtaIndeed from "@/components/cta/CtaIndeed.astro";
import CtaTwo from "@/components/cta/CtaTwo.astro";
const pageTitle = "Orange County's Direct Hire Recruiting Firm";
const pageDescription = "With over 20 years of experience, Tustin Recruiting is your direct hire firm for Orange County. We specialize in placing qualified Orange County professionals in Orange County.";
---

<BaseLayout title={pageTitle} description={pageDescription}>
  <Fragment slot="head">
    <link rel="canonical" href={new URL(Astro.url.pathname, Astro.site).toString()} />
  </Fragment>

  <HeaderOne />
  <TestimonialTwo />
  <CompanyEntries />
  <CtaTwo />
  <FeatureThree />
  <FaqTwo />
  <PricingOne />
  <CtaTwo />

</BaseLayout>
